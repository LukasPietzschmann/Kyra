struct Test {
	val a: i32;
	var b: i32;
}

val a: i32 = 1;
var b: i32 = 2;
var c: Test;

fun operator+(val a: i32, val b: Test){
	return a + b.a + b.b;
}

print a + c;

fun add(val a: i32, var b: i32): i32{
	return a + b;
}

fun add(val a: i32): i32{
	return a + 1;
}

print add(a, b);
print add(1);
